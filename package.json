        "start": "ts-node  src/main.ts",
        "start:dev": "yarn nodemon",
        "test": "yarn test:static && yarn test:unit",
        "test:static": "yarn tsc",
        "test:unit": "yarn jest --config=.jestrc.js --maxWorkers 2 --detectOpenHandles --forceExit --coverage --no-cache ",
